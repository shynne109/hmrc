                                    <xsd:annotation>
                                      <xsd:documentation>Reasonable excuse</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="H">
                                    <xsd:annotation>
                                      <xsd:documentation>Correction to earlier submission</xsd:documentation>
