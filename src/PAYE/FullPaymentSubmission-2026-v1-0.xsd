                                      <xsd:documentation>Payment subject to Class 1 NICs but P11D/P9D for tax</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="E">
                                    <xsd:annotation>
                                      <xsd:documentation>Micro Employer using temporary "on or before" relaxation</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="F">
                                    <xsd:annotation>
