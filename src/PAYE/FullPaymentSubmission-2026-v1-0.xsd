                                  <xsd:enumeration value="W2">
                                    <xsd:annotation>
                                      <xsd:documentation>Fortnightly</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="W4">
                                    <xsd:annotation>
                                      <xsd:documentation>4 Weekly</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="M1">
                                    <xsd:annotation>
                                      <xsd:documentation>Calendar Monthly</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="M3">
                                    <xsd:annotation>
                                      <xsd:documentation>Quarterly</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="M6">
                                    <xsd:annotation>
                                      <xsd:documentation>Bi-annually</xsd:documentation>
                                    </xsd:annotation>
