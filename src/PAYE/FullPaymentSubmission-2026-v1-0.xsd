                                <xsd:restriction base="xsd:string">
                                  <xsd:enumeration value="A">
                                    <xsd:annotation>
                                      <xsd:documentation>Up to 15.99 hours</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="B">
                                    <xsd:annotation>
                                      <xsd:documentation>16 to 23.99 hours</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="C">
                                    <xsd:annotation>
                                      <xsd:documentation>24 to 29.99 hours</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="D">
                                    <xsd:annotation>
                                      <xsd:documentation>30 hours or more</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="E">
                                    <xsd:annotation>
                                      <xsd:documentation>Other</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                </xsd:restriction>
