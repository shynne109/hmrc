<xsd:schema version='1.2' elementFormDefault='qualified' attributeFormDefault='unqualified' id='CISrequest' targetNamespace='http://www.govtalk.gov.uk/taxation/CISrequest' xmlns='http://www.govtalk.gov.uk/taxation/CISrequest' xmlns:xsd='http://www.w3.org/2001/XMLSchema'>
  <xsd:include schemaLocation='http://www.inlandrevenue.gov.uk/schemas/core-v2-0.xsd' />
  <xsd:annotation>
    <xsd:appinfo>
      <gms:Metadata xmlns:gms='http://www.govtalk.gov.uk/CM/gms-xs'>
        <gms:Audience>e-service developers</gms:Audience>
        <gms:Contributor>DecisionSoft Ltd</gms:Contributor>
